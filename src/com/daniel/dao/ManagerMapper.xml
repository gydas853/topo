<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.daniel.dao.ManagerMapper">

    <select id="getManager" resultType="com.daniel.model.Manager">
        select * from manager where m_name = #{m_name} and m_pass = #{m_pass};
    </select>

</mapper>
